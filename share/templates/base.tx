        </section>
      </div>
    </noscript>
    <div class="overlay"></div>
    <main class="main">
      <header>
        <: block header -> { :>
          <h1 class="sunriser">
            <a href="#">SunRiser 8</a><: $title ? " | " ~ $title : "" :>
          </h1>
          <div class="sunriser_time">
            <span>14.10.2014 - </span>21:39:39
          </div>
        <: } :>
      </header>
      <div class="page control_center">
        <: block page -> { :>
          <nav>
            <: block nav -> { :>
              <: include base::nav :>
            <: } :>
          </nav>
          <section class="sunriser_content">
            <: block content -> { :>
              Not found
            <: } :>
            <aside class="sunriser_dialogue">
              <a class="help tip" data-tip="Hilfe" href="#"><i class="fa fa-question"></i></a>
            </aside>
          </section>
        <: } :>
      </div>
